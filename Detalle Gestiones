SELECT /*+parallel (18)*/ DISTINCT 
	DELQMST.DMACCT, 
	DELQMST.DMQUE, 
	ACTFIL.ACCIDNAM, 
	ACTFIL.ACACCODE, 
	RCVRY.ACTVERTB.AVCODE, 
	ACTFIL.ACACTDTE, 
	RCVRY.ACTVERTB.AVCNTFLG, 
	RCVRY.UDA6.U6CYCDUE, 
	DELQMST.DMPAYAMT, 
	DELQMST.DMPAYDT, 
	DELQMST.DMCURBAL 
FROM 
	RCVRY.UDA6, 
	RCVRY.COLLID, 
	RCVRY.ACTVERTB, 
	RCVRY.ACTFIL, 
	RCVRY.DELQMST 
WHERE 
DELQMST.DMACCTG IN ('6','5') AND 
ACTFIL.ACACCTG IN ('6','5') AND
ACTVERTB.AVACCTG IN ('6','5') AND
UDA6.U6ACCTG IN ('6','5') AND
COLLID.CLUTYPE = '1' AND
(ACTFIL.ACCIDNAM  LIKE 'grupno%' OR 
ACTFIL.ACCIDNAM  LIKE 'sepscc%' OR
ACTFIL.ACCIDNAM  LIKE 'redme%' OR
ACTFIL.ACCIDNAM  LIKE 'redmet%' OR
ACTFIL.ACCIDNAM  LIKE 'redsb%' OR
ACTFIL.ACCIDNAM  LIKE 'gdrsa%' OR
ACTFIL.ACCIDNAM  LIKE 'gdsbb%' OR
ACTFIL.ACCIDNAM  LIKE 'redsc%' OR
ACTFIL.ACCIDNAM  LIKE 'noroc%' OR
ACTFIL.ACCIDNAM  LIKE 'gpdrs%' OR
ACTFIL.ACCIDNAM  LIKE 'sepliv%' OR
ACTFIL.ACCIDNAM  LIKE 'sepsbb%' OR
ACTFIL.ACCIDNAM  LIKE 'sepsc%' OR
ACTFIL.ACCIDNAM  LIKE 'grups%' OR
ACTFIL.ACCIDNAM  LIKE 'grupn%' OR 
ACTFIL.ACCIDNAM  LIKE 'websep%' OR
ACTFIL.ACCIDNAM  LIKE 'webnor%' OR
ACTFIL.ACCIDNAM  LIKE 'grupn%' OR
ACTFIL.ACCIDNAM  LIKE 'webred%')
AND ACTVERTB.AVTYPE = 'R'
AND	TRUNC(ACTFIL.ACACTDTE) BETWEEN '01-Oct-2025' AND '01-Nov-2025'
AND ACTFIL.ACSEQNUM = '1' 

	AND ACTFIL.ACACCT = DELQMST.DMACCT 
	AND ACTFIL.ACACCTG = DELQMST.DMACCTG 
	AND ACTFIL.ACRCCODE = ACTVERTB.AVCODE 
	AND ACTFIL.ACCIDNAM = COLLID.CLCOLLID 
	AND DELQMST.DMACCT = RCVRY.UDA6.U6ACCT 
	
	ORDER BY 6
