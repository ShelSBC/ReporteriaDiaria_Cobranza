AND AVTYPE = 'R' 
AND AVCNTFLG = 1
AND TRUNC(ACACTDTE) >= TRUNC(TO_DATE('01/01/2026','dd/mm/yyyy')) AND TRUNC(ACACTDTE) <= TRUNC(TO_DATE('31/01/2026','dd/mm/yyyy'))
AND AVCODE = 'PP'
AND ACSEQNUM = '1'
AND AVACCTG IN ('6','5')
     
AND ACTVERTB.AVACCTG=ACTFIL.ACACCTG
AND RCVRY.ACTVERTB.AVCODE = ACTFIL.ACRCCODE 
GROUP BY ACACCT) Q4
    
ON Q1.DMACCT = Q4.ACACCT
    
