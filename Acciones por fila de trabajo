SELECT /*+PARALLEL(18)*/ Q1.DMQUE,COUNT(Q1.DMACCT)CUENTAS, SUM(Q2.TOTALES)TOTALES, SUM(Q3.PRODUCTIVAS)PRODUCTIVAS, SUM(Q4.PPGENERADAS)PPGENERADAS, SUM(Q5.PPCUMPLIDAS)PPCUMPLIDAS
FROM
    
(SELECT DMACCT, DMQUE
FROM RCVRY.DELQUDA_BEGIN
WHERE TRUNC(BEGINDT) = TRUNC(TO_DATE('01/01/2026','dd/mm/yyyy'))
AND DMQUE IN ('001E','001P','001S','001U','001Y','002P','002S','002U','002Y','003C','003L','003R','003W','004C','004K','004L','004R','004T',
